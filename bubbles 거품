import random

f = 4
a = []


def list():
    for t in range(f):
        r = random.randrange(10)
        a.append(r)
    return a


list = list()
print(list)


def q():
    for l in range(f):
        for i in range(f - 1):
            if a[i] > a[i + 1]:
                a[i], a[i + 1] = a[i + 1], a[i]
                print(a)
            if a[i] == a[i + 1]:
                break


q()
