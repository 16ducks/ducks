import random
a = []

def list():
    for t in range(10):
        r = random.randrange(100)
        a.append(r)
    return a
g = list()
print(g)
def sort():
    for i in range(10):
        for j in range(9):
            if a[j] > a[j+1]:
                a[j], a[j+1] = a[j+1], a[j]
                print(a)
    return a
c = sort()
print(c)

